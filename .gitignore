!.idea/codeStyles/**

!.idea/inspectionProfiles/
!.idea/inspectionProfiles/**

!.idea/runConfigurations/
!.idea/runConfigurations/**

# still ignore user-specific stuff
.idea/workspace.xml
.idea/tasks.xml
.idea/shelf/
.idea/httpRequests/

